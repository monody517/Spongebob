ate(-50%, -50%); /* x,y轴移动 */\r\n  width: 40px;\r\n  height: 40px;\r\n  background: #333;\r\n  border-radius: 50%;\r\n}</style>\r\n</head>\r\n<body>\r\n    <div class=\"face\"> <!-- 脸部 -->\r\n        <div class=\"eyes\"> <!--  眼睛 -->\r\n            <div class=\"eye\"></div>\r\n            <div class=\"eye\"></div>\r\n        </div>\r\n    </div>\r\n    <script>// 给body添加鼠标移动事件 eyeball\ndocument.querySelector('body').addEventListener('mousemove', eyeball); // 事件函数\n\nfunction eyeball() {\n  // 根据css选择器 获取到元素 放入数组\n  var eye = document.querySelectorAll('.eye'); // 调用数组中的每个元素\n\n  eye.forEach(function (eye) {\n    // 元素相对于窗口左边的距离  左边框的宽度\n    var x = eye.getBoundingClientRect().left + eye.clientLeft / 2; // 同上\n\n    var y = eye.getBoundingClientRect().top + eye.clientTop / 2; // pageX 鼠标指针的位置，相对于文档左边缘\n\n    var radian = Math.atan2(event.pageX - x, event.pageY - y);\n    var rot = radian * (180 / Math.PI) * -1 + 270; // 修改旋转角度\n\n    eye.style.transform = 'rotate(' + rot + 'deg)';\n  });\n}</script>\r\n</body>\r\n</html>"},"sourceMaps":null,"error":null,"hash":"4d273a4ecc9923e63eb01142bf0ccf81","cacheData":{"env":{}}}